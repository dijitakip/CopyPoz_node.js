# Production Environment Variables
# Hostinger'da bu değerleri ayarlayın

# Database Configuration (Hostinger MySQL)
DATABASE_URL="mysql://username:password@hostname:3306/database_name"

# Master Token (Güvenli bir token kullanın)
MASTER_TOKEN="your_secure_master_token_here"

# Session Secret (Güvenli bir secret kullanın)
SESSION_SECRET="your_session_secret_here"

# Node Environment
NODE_ENV="production"

# Port (Hostinger otomatik yönlendirir)
PORT=3000

# Next.js Public Variables
NEXT_PUBLIC_MASTER_TOKEN="your_secure_master_token_here"

# Security Headers
NEXT_PUBLIC_API_URL="https://yourdomain.com"
